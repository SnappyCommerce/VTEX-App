{
  "name": "snappy-commerce",
  "vendor": "snappybotsar",
  "version": "0.1.0",
  "title": "Snappy bot installer",
  "description": "This app let's you inject the snappy bot on your vtex page",
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true,
    "type": "free",
    "availableCountries": ["*"]
  },
  "builders": {
    "react": "3.x",
    "store": "0.x",
    "pixel": "0.x",
    "docs": "0.x"
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x"
  },
  "settingsSchema": {
    "title": "SnappyCommerce",
    "type": "object",
    "properties": {
      "botUuid": {
        "title": "Bot UUID",
        "description": "Enter your Bot UUID",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
